   <Key mask="CAS" key="Left">sendl</Key>
   <Key mask="CAS" key="Right">sendr</Key>
   <Key mask="CAS" key="Up">sendu</Key>
   <Key mask="CAS" key="Down">sendd</Key>   
   <Key mask="CA" key="Right">rdesktop</Key>
   <Key mask="CA" key="Left">ldesktop </Key>
   <Key mask="CA" key="Up">udesktop</Key>
