<externaltool id="Колончатое_форматирование">
    <description>Колончатое форматирование выделеного текста</description>
    <displayname>Колончатое форматирование</displayname>
    <category>Text</category>
    <executable output="replaceselection" error="ignore" modifiesdocument="no">
        <path>column</path>
        <arguments>-t -o &quot; &quot;</arguments>
        <input>%{CurrentDocument:Selection}</input>
        <workingdirectory>%{CurrentDocument:Path}</workingdirectory>
    </executable>
</externaltool>
